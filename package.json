{
  "name": "blpf",
  "displayName": "BLPF",
  "description": "Adds support for the BLPF programming language.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.99.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
  "languages": [
    {
      "id": "blpf",
      "aliases": ["BLPF Assembly", "blpf"],
      "extensions": [".blpf"],
      "configuration": "./language-configuration.json"
    }
  ],
  "grammars": [
    {
      "language": "blpf",
      "scopeName": "source.blpf",
      "path": "./syntaxes/blpf.tmLanguage.json"
    }
  ]
}

}
